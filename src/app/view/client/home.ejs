<section class="slider_section">
    <div id="myCarousel" class="carousel slide banner-main" data-ride="carousel">
        <div class="carousel-inner">
            <% let active = true; data.slider.map((image)=>{ %>
                <div class="carousel-item <%= active ? 'active' : '' %>">
                    <img class="first-slide" src="/assets/media/website/<%= image.name %>" alt="First slide">
                </div>
            <% active = false}); %>
        </div>
        <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
            <i class='fa fa-angle-left'></i>
        </a>
        <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
            <i class='fa fa-angle-right'></i>
        </a>
    </div>
</section>
<!-- about -->
<div class="about">
    <div class="container">
        <div class="row">
            <div class="col-xl-5 col-lg-5 col-md-5 co-sm-l2">
                <div class="about_img">
                    <figure><img src="/assets/media/website/<%= data.about.thumbnail %>" alt="img"/></figure>
                </div>
            </div>
            <div class="col-xl-7 col-lg-7 col-md-7 co-sm-l2">
                <div class="about_box">
                    <h3>About Us</h3>
                    <span>Our Mobile Shop</span>
                    <p><%= data.about.introduction %></p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end about -->
<!-- brand -->
<div class="brand">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="titlepage">
                    <h2>Our Brand</h2>
                </div>
            </div>
        </div>
    </div>
    <%- include('../partials/client/brand') %>
</div>
<%- contentFor('script') %>

<%- contentFor('style') %>